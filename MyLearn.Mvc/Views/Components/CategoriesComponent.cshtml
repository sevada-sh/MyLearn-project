@model IEnumerable<MyLearn.Domain.Models.Category>

<nav class="navbar navbar-expand-lg navbar-light bg-light text-nowrap category" dir="rtl">
    <div class="container">
        <div class="collapse navbar-collapse" id="navbarText">
            <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                @foreach (var item in Model)
                {
                    <li class="nav-item">
                        <a class="nav-link px-3 active" aria-current="page" href="#">@item.CategoryName</a>
                    </li>
                }
            </ul>
        </div>
    </div>
</nav>